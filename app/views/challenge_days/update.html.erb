<%= turbo_frame_tag dom_id(@challenge_day) do %>
  <% if @challenge_day.success? %>
    <%= render(ChallegeDayCompletedComponent.new(challenge_day: @challenge_day)) %>
  <% elsif @challenge_day.fail? %>
    <%= render(::ChallengeDay::ChallengeDayFailComponent.new(challenge_day: @challenge_day)) %>
  <% end %>
<% end %>
